git config --global credential.helper cache
#Set git to use the credential memory cache


git config --global credential.helper 'cache --timeout=3600'
#Set the cache to timeout after 1hour(setting is in seconds)

git remote
git remote add origin git@github.com:cbuncky-uda/reflections.git

git remote
#it will show origin

git remote -v

git push origin master
#master:my repo on github
